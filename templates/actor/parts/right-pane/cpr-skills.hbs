<div class="skills-tab">
    <ol class="left-pane-skills items-list">
        {{#each (findConfigObj "skillCategories") as |category c|}}
        {{#if (compare c "<" "f")}}
            {{> "systems/cyberpunk-red-core/templates/actor/parts/right-pane/parts/skills/cpr-skills-category.hbs"  (mergeForPartialArg "category,actor" c ../this.actor)}}
        {{/if}}
        {{/each}}
    </ol>
    <ol class="right-pane-skills items-list">
        {{#each (findConfigObj "skillCategories") as |category c|}}
        {{#if  (compare c ">=" "f")}}
            {{> "systems/cyberpunk-red-core/templates/actor/parts/right-pane/parts/skills/cpr-skills-category.hbs"  (mergeForPartialArg "category,actor" c ../this.actor)}}
        {{/if}}
        {{/each}}
    </ol>
</div>