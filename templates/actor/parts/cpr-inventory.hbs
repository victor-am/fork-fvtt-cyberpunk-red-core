{{log "CPR-INVENTORY.HBS"}}
<div class="tab-block">
  <table>
    <tbody>
      {{#each (findConfigObj "inventoryCategories") as |category c|}}
      
        {{> (generatePartial "systems/cyberpunk-red-core/templates/actor/parts/common/inventory/cpr-VAR-inventory-header.hbs" c)}}
          {{#each ../actor.items as |item i|}}
            {{#if (eq item.type c)}}
              {{> (generatePartial "systems/cyberpunk-red-core/templates/actor/parts/common/inventory/cpr-VAR-inventory-content.hbs" c)}}
            {{/if}}
          {{/each}}
      {{/each}}
    </tbody>
  </table>
</div>
{{log "END CPR-INVENTORY.HBS"}}