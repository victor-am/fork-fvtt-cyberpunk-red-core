{{log "CPR-CYBERWARE-HBS"}}
<div class="dynamic-list cyberware">
    {{#each (findConfigObj "cyberwareTypeList") as |type t|}}
    <div class="row header black-bg white-fg bold inventory">
        <span class="section name width-quarter">{{localize type}}</span>
        <span class="section data width-grow">{{localize "CPR.data"}}</span>
        <span class="section action width-quarter right-text">{{localize "CPR.actions"}}</span>
    </div>
    {{!-- Revist...? --}}
    {{> "systems/cyberpunk-red-core/templates/actor/parts/common/cyberware/cpr-cyberware-content.hbs" (getProp ../this.installedCyberware t)}}
    {{/each}}
</div>
{{log "END CPR-CYBERWARE-HBS"}}