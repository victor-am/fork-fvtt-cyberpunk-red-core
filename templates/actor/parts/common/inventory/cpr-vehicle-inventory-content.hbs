{{log "CPR-WEAPON-INV-CONTENT.HBS"}}
<tr class="item" data-item-id={{this._id}}>
    <td class="name"><a class="name">
            {{this.name}}</a>
        <a class="item-edit"><i class="fas fa-edit"></i></a>
        <a class="item-delete"><i class="fas fa-trash-alt"></i></a>
    </td>
    <td class="item inventory-center">{{this.data.seats}}</td>
    <td class="item inventory-center">
        {{this.data.spd}}/{{this.data.spd}}
    </td>
    <td class="item inventory-center">
        {{> "systems/cyberpunk-red-core/templates/actor/parts/common/cpr-equip-glyph.hbs"}}
    </td>
</tr>
{{log "END CPR-WEAPON-INV-CONTENT.HBS"}}