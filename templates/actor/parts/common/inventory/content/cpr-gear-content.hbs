{{log "CPR-GEAR-INV-CONTENT.HBS"}}
<div class="row inventory item" data-item-id={{item._id}}>
    <div class="section name">
        <a class="name item-edit">{{item.name}}</a>
    </div>
    <div class="section data">
        {{localize "CPR.price"}}: {{item.data.price.market}}
    </div>
    <div class="section action">
        {{> "systems/cyberpunk-red-core/templates/actor/parts/common/inventory/actions/cpr-actions.hbs" item}}
    </div>
</div>
{{log "END CPR-GEAR-INV-CONTENT.HBS"}}