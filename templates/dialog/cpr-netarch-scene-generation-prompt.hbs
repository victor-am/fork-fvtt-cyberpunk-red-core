<form>
  <ol class="items-list">
    <li class="item flexrow">
        <div class="item flexrow setting-name">
          <span class="text-bold text-nowrap">Setting</span>
        </div>
        <div class="item flexrow setting-value">
          <span class="text-bold text-nowrap text-center text-space-around">Value</span>
        </div>
    </li>
    <hr>
    <li class="item flexrow">
        <div class="item flexrow setting-name">
          <span>
            <div class="text-bold">{{localize "CPR.prompt.netArchitectureGeneration.animate"}}</div>
            <div style="font-size: 0.7rem">{{localize "CPR.prompt.netArchitectureGeneration.animateHint"}}</div>
          </span>
        </div>
        <div class="item flexrow setting-value">
          <span class="text-nowrap text-center text-space-around"><input type="checkbox" name="animated" value="{{animated}}"></span>
        </div>
    </li>
    <hr>
    <div class="text-bold fg-red">{{localize "CPR.prompt.netArchitectureGeneration.warning"}}</div>
    <hr>
    <li class="item flexrow">
        <div class="item flexrow setting-name">
          <span>
            <div class="text-bold">{{localize "CPR.prompt.netArchitectureGeneration.tilesCustom"}}</div>
            <div style="font-size: 0.7rem">{{localize "CPR.prompt.netArchitectureGeneration.tilesCustomHint"}}</div>
          </span>
        </div>
